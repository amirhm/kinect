wget http://www.openni.org/wp-content/uploads/2013/01/OpenNI-MacOS-x64-2.1.0.tar.zip
wget http://www.openni.org/wp-content/uploads/2013/02/NiTE-MacOS-x64-2.0.0.tar1.zip

echo unzipping
unzip NiTE-MacOS-x64-2.0.0.tar1.zip
unzip OpenNI-MacOS-x64-2.1.0.tar.zip

echo untarring
tar -jxf NiTE-MacOS-x64-2.0.0.tar.bz2
tar -jxf OpenNI-MacOS-x64-2.1.0.tar.bz2

echo removing tars
rm NiTE-MacOS-x64-2.0.0.tar.bz2
rm OpenNI-MacOS-x64-2.1.0.tar.bz2
